{
  "name": "angular2-preboot",
  "version": "1.1.2",
  "description": "A teeny tiny Angular 2 template to get your next great project booted. It is powered by great ideas and Webpack 2.",
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "author": {
    "name": "Sebastian DÃ¶ll",
    "email": "sebastian@katallaxie.me"
  },
  "homepage": "https://github.com/katallaxie/angular2-preboot",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-preboot:latest .",
    "build:prod": "cross-env NODE_ENV=production webpack  --progress --profile --bail --debug",
    "build": "npm run build:dev",
    "ci": "npm test && npm run e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:aot": "npm run rimraf --aot",
    "clean:install": "npm set progress=false && npm install",
    "clean:dll": "rimraf -- dll",
    "clean:start": "npm run clean:dll && npm run clean:dist && start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist aot dll",
    "cross-env": "cross-env",
    "dll": "cross-env NODE_ENV=development webpack --progress --profile",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "lint": "npm run tslint 'src/**/*.ts'",
    "postversion": "git push && git push --tags",
    "prebuild:aot": "npm run clean:dist",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "preversion": "npm test",
    "protractor": "protractor",
    "prod": "cross-env NODE_ENV=production",
    "rimraf": "rimraf",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server --progress --profile --watch",
    "server:prod": "http-server dist --cors",
    "server": "npm run server:dev",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=testing npm run lint && karma start",
    "tslint": "node_modules/tslint/bin/tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/router": "3.4.3",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "rxjs": "5.0.1",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@angular/compiler": "2.4.3",
    "@angular/compiler-cli": "2.4.3",
    "@angular/language-service": "^2.4.3",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.40",
    "@types/node": "^7.0.0",
    "@types/selenium-webdriver": "^2.53.37",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.0.27",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.6.1",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "codelyzer": "~2.0.0-beta.4",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.0",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.25.0",
    "imports-loader": "^0.7.0",
    "istanbul-instrumenter-loader": "^1.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-coverage": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "ng-router-loader": "^1.0.2",
    "ngc-webpack": "^1.1.0",
    "parse5": "^3.0.1",
    "postcss": "^5.2.8",
    "postcss-loader": "^1.2.2",
    "protractor": "^5.0.0",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.2",
    "script-ext-html-webpack-plugin": "^1.4.2",
    "source-map-loader": "^0.1.6",
    "string-replace-loader": "1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "1.1.2",
    "ts-node": "^2.0.0",
    "tslint": "^4.1.1",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.0",
    "typescript": "2.1.5",
    "url-loader": "^0.5.7",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.2.0-rc.4",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katallaxie/angular2-preboot.git"
  },
  "bugs": {
    "url": "https://github.com/katallaxie/angular2-preboot/issues"
  },
  "engines": {
    "node": ">= 6.9.1",
    "npm": ">= 3"
  }
}
